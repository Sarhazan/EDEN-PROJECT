{
  "name": "eden-maintenance",
  "version": "1.0.0",
  "description": "Comprehensive maintenance management system with WhatsApp integration",
  "main": "server/index.js",
  "scripts": {
    "dev:server": "nodemon server/index.js",
    "dev:client": "cd client && npm run dev",
    "dev:client:maintenance": "cd client && npm run dev -- --port 5174 --strictPort",
    "dev:client:hq": "cd client && npm run dev -- --port 5175 --strictPort",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:all:fixed-ports": "concurrently \"npm run dev:server\" \"npm run dev:client:maintenance\" \"npm run dev:client:hq\"",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install:all": "npm install && cd client && npm install"
  },
  "keywords": [
    "maintenance",
    "management",
    "tasks",
    "whatsapp",
    "facilities",
    "hebrew",
    "rtl"
  ],
  "author": "Eden Team",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@google-cloud/translate": "^9.3.0",
    "@google/generative-ai": "^0.24.1",
    "@sparticuz/chromium": "^143.0.4",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.6.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "i18next": "^25.8.0",
    "i18next-fs-backend": "^2.6.1",
    "jsdom": "^27.4.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "puppeteer-core": "^24.36.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.3",
    "whatsapp-web.js": "^1.34.4"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11"
  }
}

{
  "version": 2,
  "name": "eden-task-pages",
  "builds": [
    {
      "src": "docs/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/task-(.*).html",
      "dest": "/docs/task-$1.html"
    },
    {
      "src": "/(.*)",
      "dest": "/docs/$1"
    }
  ],
  "github": {
    "enabled": true,
    "autoAlias": true
  }
}
